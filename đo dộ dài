<script> 
  var map = L.map('map').setView([10.0364634, 105.7875821], 7);


  L.tileLayer('https://tile.openstreetmap.org/{z}/{x}/{y}.png', {
    maxZoom: 19,
    attribution: '&copy; <a href="http://www.openstreetmap.org/copyright">OpenStreetMap</a>'
}).addTo(map);
var maker;
function onMapClick(e) {
    if (maker != undefined) {
    map.removeLayer(maker);
  };
    var myIcon = L.icon({
    iconUrl: 'https://scontent.fsgn5-12.fna.fbcdn.net/v/t39.30808-6/438303812_959090869332762_1589647337058305618_n.jpg?stp=cp6_dst-jpg&_nc_cat=103&cc...647B939',
    iconSize: [38, 95],
    iconAnchor: [22, 94],
    popupAnchor: [-3, -76],
    shadowUrl: 'my-icon-shadow.png',
    shadowSize: [68, 95],
    shadowAnchor: [22, 94]
});

 maker=L.marker(e.latlng, {icon: myIcon,'draggable':true}).addTo(map);
maker.bindPopup("Hello DiAn");
}

map.on('click', onMapClick);



<!--tao maker hinh, di chuyen maker, hien chu, bam nay mat kia-->


</script>
  </body>
</html>
